#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i;
    int n=1;

    scanf("%d", &n);
    
    for(int i=1; i<=n; ++i){
        if (i==1)
        {
            printf("%dst\n", i);
        }
        else if (i==2)
        {
            printf("%dnd\n", i);
        }   
        else if (i==3)
        {
            printf("%drd\n", i);
        }
        else 
        {
            printf("%dth\n", i);
        }
    }


    return 0;
}
