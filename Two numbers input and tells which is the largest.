#include <stdio.h>

 int main()
 {
 float x, y;
 printf("Please enter a number: ");
 scanf("%f", &x);
 printf("Please enter another number: ");
 scanf("%f", &y);
 if(x > y) {
 printf("The largest number entered was: %f\n", x);
 } else if(y > x) {
 printf("The largest number entered was: %f\n", y);
 } else {
 printf("The numbers entered are equal.");
 }
 return 0;
 }
